{
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.refreshOnInsertMode": true,
  "eslint.autoFixOnSave": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.showUnused": true,
  "typescript.showDeprecated": true,
  "go.goplsOptions": {
	  "gofumpt": true,
	  "env": {
		  "GOFLAGS": "--tags=\"test,unit,e2e,it\""
	  }
  },
  "cSpell.userWords": [
    "Asen",
    "Certbot",
    "DNSIP",
    "Enthys",
    "Mihaylov",
    "Nikola",
    "Prodanov",
    "Redlock",
    "SYNCERROR",
    "Vhost",
    "Vhosts",
    "Vmin",
    "XDNSSL",
    "amqp",
    "barzaz",
    "commitlint",
    "desync",
    "dtos",
    "epub",
    "eqeqeq",
    "fenix",
    "fenixmq",
    "gonic",
    "gorm",
    "healthcheck",
    "hostbill",
    "iprodanov",
    "microservices",
    "mitev",
    "nestjs",
    "noout",
    "prebuild",
    "rabbitmq",
    "recard",
    "streadway",
    "timestamptz",
    "typeorm",
    "varchar",
    "wbst"
  ],
  "snippets.ultisnips.pythonPrompt": false,
  "languageserver": {
	  "golang": {
		  "command": "gopls",
		  "rootPatterns": ["go.mod", ".git/"],
		  "disableWorkspaceFolders": true,
		  "filetypes": ["go"],
		  "initializationOptions": {
			  "completeUnimported": true
		  },
	  }
  },
}
